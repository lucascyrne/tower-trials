{
  "name": "tower-trials",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:clean": "npm run sw:unregister && rm -rf dist && rm -rf .vite && vite --host --force --clearScreen false",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "# Desenvolvimento": "",
    "sw:unregister": "node scripts/unregister-sw.js",
    "# Supabase Local": "",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:status": "supabase status",
    "# Gerenciamento de Ambientes": "",
    "env:local": "bash ./scripts/setup-local-env.sh",
    "env:dev": "bash ./scripts/setup-dev-env.sh",
    "# Workflows Completos": "",
    "local": "npm run env:local && npm run db:start && npm run dev",
    "dev-remote": "npm run env:dev && npm run dev",
    "# Migrações Seguras": "",
    "migrate:local": "bash ./scripts/apply-migrations.sh local",
    "migrate:dev": "bash ./scripts/apply-migrations.sh dev",
    "# PWA": "",
    "pwa:icons": "node scripts/generate-pwa-icons.js",
    "pwa:serve": "npm run build && npm run start",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-router": "^1.120.18",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-devtools": "^1.120.18",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.6.3",
    "lucide-react": "^0.513.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.6.7",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tanstack/router-vite-plugin": "^1.120.19",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "terser": "^5.42.0",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
